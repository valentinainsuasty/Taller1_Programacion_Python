print("Ingrese el primer numero entero: ")
n1 = int(input())
print("Ingrese el segundo numero entero: ")
n2 = int(input())

cociente = num1 / num2
residuo = num1 % num2 == 0

print(f"La division de {num1} entre {num2} es {cociente:.2f}")
if residuo:
    print("La division es exacta.")
else:
    print("La division no es exacta.")